    "ramda-lens": "^0.1.2",
    "react": "^16.4.0",
    "react-apollo": "^2.1.4",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.4.0",
    "react-map-gl": "^3.2.7",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scrollbar": "^0.5.4",
    "rebass": "^1.0.7",
    "redux": "^4.0.0",
    "redux-debounced": "^0.5.0",
    "redux-logger": "^3.0.6",
    "redux-map-gl": "^0.1.0",
    "redux-mock-provider": "^1.0.0",
