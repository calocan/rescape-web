{
  "name": "rescape-web",
  "description": "Demo project that incorporates all the Rescape libraries",
  "license": "MIT",
  "version": "0.1.4",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "lib",
    "es",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calocan/rescape-web"
  },
  "author": "Andy Likuski andy@likuski.org",
  "bugs": {
    "url": "https://github.com/calocan/rescape-web"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "startTest": "NODE_ENV=test node scripts/start.js",
    "buildCreateReactApp": "node scripts/build.js",
    "clean": "rimraf lib dist es coverage",
    "lint": "eslint src",
    "test": "cross-env BABEL_ENV=commonjs jest",
    "test:watch": "yarn test -- --watch",
    "test:cov": "yarn test -- --coverage",
    "build:commonjs": "cross-env NODE_ENV=production BABEL_ENV=commonjs  node_modules/.bin/babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es NODE_ENV=es node_modules/.bin/babel src --out-dir es",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development $NODE_DEBUG_OPTION rollup -c -i src/index.js -o dist/index.js",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production NODE_ENV=production rollup -c -i src/index.js -o dist/index.min.js",
    "build": "yarn run build:commonjs && yarn run build:es && yarn run build:umd && yarn run build:umd:min",
    "prepare": "yarn run clean && yarn run lint && yarn test && yarn run build",
    "docs:clean": "rimraf _book",
    "docs:prepare": "gitbook install",
    "docs:build": "yarn run docs:prepare && gitbook build -g calocan/rescape-web && cp logo/apple-touch-icon.png _book/gitbook/images/apple-touch-icon-precomposed-152.png && cp logo/favicon.ico _book/gitbook/images",
    "docs:watch": "yarn run docs:prepare && gitbook serve",
    "docs:publish": "yarn run docs:clean && yarn run docs:build && cp CNAME _book && cd _book && git init && git commit --allow-empty -m 'update book' && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am 'update book' && git push git@github.com:reactjs/redux gh-pages --force",
    "build-publish-patch": "yarn run build && yarn run publish-patch",
    "build-publish-minor": "yarn run build && yarn run publish-minor",
    "publish-patch": "yarn --new-version version patch && yarn publish",
    "publish-minor": "yarn --new-version version minor && yarn publish"
  },
  "dependencies": {
    "@turf/bbox-polygon": "^6.0.0",
    "@turf/turf": "^5.1.6",
    "ansi-styles": "^3.2.1",
    "apollo": "^0.2.2",
    "apollo-client-preset": "^1.0.8",
    "apollo-link": "^1.0.7",
    "apollo-link-error": "^1.0.3",
    "apollo-link-schema": "^1.0.2",
    "apollo-link-ws": "^1.0.4",
    "autoprefixer": "8.1.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "22.4.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-graphql-tag": "^1.3.1",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "babel-preset-env": "^1.6.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "2.3.2",
    "cors": "^2.8.4",
    "css-loader": "0.28.10",
    "d3-array": "^1.2.1",
    "d3-collection": "^1.0.4",
    "d3-format": "^1.2.2",
    "d3-geo": "^1.9.1",
    "d3-sankey": "../d3-sankey/",
    "d3-scale": "^2.0.0",
    "data.either": "^1.5.1",
    "data.maybe": "^1.2.2",
    "data.task": "^3.1.1",
    "decamelize": "^2.0.0",
    "dotenv": "5.0.1",
    "eslint": "^4.18.2",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "2.46.1",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "fs-extra": "5.0.0",
    "google-spreadsheets": "^1.0.0",
    "googleapis": "^27.0.0",
    "graphql-geojson": "^1.0.0",
    "graphql-tag": "^2.7.0",
    "graphql-tools": "^2.20.0",
    "html-webpack-plugin": "3.0.4",
    "immutable": "^3.8.2",
    "jest": "^22.1.4",
    "memoize-immutable": "^3.0.0",
    "moment": "^2.21.0",
    "moment-duration-format": "^2.2.1",
    "morgan": "^1.9.0",
    "namedtuplemap": "^1.0.0",
    "node-fetch": "^2.1.1",
    "numeral": "^2.0.6",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-loader": "2.1.1",
    "pretty-format": "^22.1.0",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
    "query-overpass": "^1.5.3",
    "raf": "3.4.0",
    "ramda": "^0.25.0",
    "ramda-lens": "^0.1.2",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.2.0",
    "react-map-gl": "^3.2.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scrollbar": "^0.5.1",
    "rebass": "^1.0.4",
    "redux": "^3.7.2",
    "redux-debounced": "^0.4.0",
    "redux-logger": "^3.0.6",
    "redux-map-gl": "^0.1.0",
    "redux-mock-provider": "^1.0.0",
    "redux-mock-store": "^1.5.1",
    "redux-responsive": "^4.3.5",
    "redux-thunk": "^2.2.0",
    "rescape-cycle": "^1.6.2",
    "rescape-helpers": "^1.1.25",
    "rescape-helpers-component": "^1.4.20",
    "rescape-ramda": "^1.4.23",
    "rescape-sample-data": "^1.2.12",
    "rescape-validate": "^1.2.7",
    "reselect": "^3.0.1",
    "style-loader": "0.20.2",
    "styled-components": "^3.1.6",
    "styled-system": "^2.1.2",
    "subscriptions-transport-ws": "^0.9.5",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "1.0.1",
    "webpack": "^4.1.0",
    "webpack-dev-server": "3.1.0",
    "webpack-manifest-plugin": "2.0.0-rc.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "apollo-test-utils": "^0.3.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "circular-dependency-plugin": "^4.4.0",
    "cross-env": "^5.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "enzyme-wait": "^1.0.9",
    "install": "^0.10.4",
    "jest-sourcemaps": "^1.0.1",
    "jsdom": "^11.6.2",
    "longjohn": "^0.2.12",
    "nock": "^9.1.6",
    "react-mock-router": "^1.0.11",
    "redux-devtools-extension": "^2.13.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.56.4",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.4.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "resolutions": {},
  "jest": {
    "modulePaths": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jestsetup.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "ie >= 11"
            ]
          },
          "exclude": [
            "transform-async-to-generator"
          ],
          "loose": false
        }
      ]
    ],
    "sourceMaps": true,
    "plugins": [
      [
        "transform-es2015-modules-commonjs-simple",
        {
          "noMangle": true
        }
      ],
      "transform-object-rest-spread",
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint"
  },
  "peerDependencies": {
    "graphql": "^0.13.1"
  }
}
